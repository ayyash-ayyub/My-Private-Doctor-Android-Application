<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/back"
    >
    
    <RelativeLayout
    android:orientation="vertical" 
    android:layout_width="fill_parent"
    android:layout_height="200px"
    android:layout_marginTop="100px"
    >
	
	
    <TextView
    android:id="@+id/tview"
    android:textColor="#f0f0f0"
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content"
    android:text="Masukkan Gejala Penyakit"
    android:textSize="12px"
	android:layout_marginLeft="70px"
    ></TextView>
    
	<AutoCompleteTextView  
	android:layout_below="@+id/tview" 
	android:layout_marginTop="-2px"
    android:id="@+id/auto"
    android:layout_centerHorizontal="true"
    android:layout_width="210px" 
    android:layout_height="wrap_content" 
    android:background="@android:drawable/title_bar"
    android:singleLine="true"
    />
    <Button
    android:layout_marginTop="25px"
    android:layout_toLeftOf="@+id/auto"
    android:id="@+id/tombol_back"
    android:background="@drawable/tombol_back_xml"
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
    ></Button>
    
    <Button
    android:layout_marginTop="20px"
    android:layout_below="@+id/auto"
    android:layout_centerHorizontal="true"
    android:id="@+id/tombol"
    android:background="@drawable/alphabetclick"
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content" 
    ></Button>
    </RelativeLayout>
    
    <RelativeLayout
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent"
     
    >
    
    
    </RelativeLayout>
    
    
    
</LinearLayout>
